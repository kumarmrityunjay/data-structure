#include<stdio.h>
#include<math.h>
int main(){
 int n;
 int i,count=0,j=0;
 printf("Enter the position:\n");
 scanf("%d",&n);
 int a[n];
 for(i=0;i<n;i++){
    a[i]=(pow(2,j));
    i=i+1;
    a[i]=(pow(3,j));
    j=j+1;
    count++;

   }
 for(i=0;i<n;i++){
     printf("%d\t",a[i]);
  }
  printf("\n");
  printf("The %d th position element  is the %d \n",(n) ,a[n-1]);


 }
